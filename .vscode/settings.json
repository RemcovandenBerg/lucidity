{
    "editor.tabCompletion": true,
    "editor.quickSuggestions": {
        "other": false,
        "comments": true,
        "strings": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/obj": true,
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/obj": true
    },
    "terminal.integrated.env.windows": {
        "ASPNETCORE_ENVIRONMENT": "Development"
    },
}
